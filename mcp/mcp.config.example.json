{
  "mcpServers": {
    "supabase": {
      "command": "mcp-supabase",
      "env": ["SUPABASE_URL", "SUPABASE_ANON_KEY"]
    },
    "vercel": {
      "command": "mcp-vercel",
      "env": ["VERCEL_TOKEN"]
    },
    "cloudflare": {
      "command": "mcp-cloudflare",
      "env": ["CLOUDFLARE_API_TOKEN", "CLOUDFLARE_ACCOUNT_ID"]
    }
  }
}


